{
  "name": "78778443/qingscan",
  "description": "Qingscan安全运营工作台",
  "type": "project",
  "keywords": [
    "security",
    "scan",
    "qingscan",
    "thinkphp"
  ],
  "homepage": "https://github.com/78778443/qingscan",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "daxia",
      "email": "78778443@qq.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "topthink/framework": "^8.1",
    "topthink/think-orm": "^4.0",
    "topthink/think-view": "^2.0",
    "xaboy/form-builder": "^2.1",
    "rmccue/requests": "*",
    "jeremykendall/php-domain-parser": "^6.1",
    "ext-json": "*",
    "topthink/think-multi-app": "^1.1",
    "ext-curl": "*",
    "zoujingli/ip2region": "^2.0",
    "symfony/yaml": "^6.3",
    "symfony/http-foundation": "^6.0",
    "topthink/think-migration": "^3.1",
    "ecloudsdk/tag": "1.0.0",
    "ecloud/ecloud-sdk-core": "*",
    "ecloudsdk/ecs": "*",
    "volcengine/volcengine-php-sdk": "v1.0.94"
  },
  "require-dev": {
    "symfony/var-dumper": "^4.2",
    "topthink/think-trace": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "app\\": "app"
    },
    "psr-0": {
      "": "extend/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "allow-plugins": {
      "elendev/composer-push": true
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php think service:discover",
      "@php think vendor:publish"
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://ecloud.10086.cn/api/query/developer/nexus/repository/composer/"
    }
  ]
